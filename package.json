{
  "name": "sfsf-projman",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "ECEmployeeProfile": {
        "kind": "odata-v2",
        "model": "srv/external/ECEmployeeProfile",
        "credentials":{
          "destination":"SFSF",
          "path": "/odata/v2/Background_SpecialAssign"
        }
      },
      "PLTUserManagement": {
        "kind": "odata-v2",
        "model": "srv/external/PLTUserManagement",
        "credentials":{
          "destination":"SFSF",
          "path": "/odata/v2/User"
        }
      }
    }
  }
}